package cn.wellcare.core.constant;

/**
 * 订单操作
 * 
 * @author zhaihl
 *
 */
public enum OrderHandler {
	CREATE("创建订单"), UPDATE("更新订单"), AUTO_CANCEL("系统自动取消");

	private String name;

	private OrderHandler(String name) {
		this.setName(name);
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
