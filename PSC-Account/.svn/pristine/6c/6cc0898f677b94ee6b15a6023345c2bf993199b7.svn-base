<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace和dao对应  -->
<mapper namespace="cn.wellcare.dao.acc.PscPiAccDao">
    <resultMap id="PscPiAccResult" type="cn.wellcare.acc.entity.PscPiAcc">
        <result property="amtAcc" column="amt_acc"/>
        <result property="codeAcc" column="code_acc"/>
        <result property="createTime" column="create_time"/>
        <result property="creator" column="creator"/>
        <result property="creditAcc" column="credit_acc"/>
        <result property="delFlag" column="del_flag"/>
        <result property="euStatus" column="eu_status"/>
        <result property="modifier" column="modifier"/>
        <result property="modityTime" column="modity_time"/>
        <result property="note" column="note"/>
        <result property="pkPi" column="pk_pi"/>
        <result property="pkPiacc" column="pk_piacc"/>
        <result property="unavailableAcc" column="unavailable_acc"/>
        <result property="pwd" column="pwd"/>
    </resultMap>

    <select id="getAccount" parameterType="string"
            resultMap="PscPiAccResult">
		select
			amt_acc,
			code_acc,
			create_time,
			creator,
			credit_acc,
			del_flag,
			eu_status,
			modifier,
			modity_time,
			note,
			pk_pi,
			pwd,
			pk_piacc,
			unavailable_acc
		from psc_pi_acc
		where pk_pi = #{pkPi}
	</select>
    <select id="getAccountBycode" parameterType="string"
            resultMap="PscPiAccResult">
		select
			amt_acc,
			code_acc,
			create_time,
			creator,
			credit_acc,
			del_flag,
			eu_status,
			modifier,
			modity_time,
			note,
			pk_pi,
			pwd,
			pk_piacc,
			unavailable_acc
		from psc_pi_acc
		where code_acc = #{codeAcc}
	</select>

    <select id="get" parameterType="Integer"
            resultMap="PscPiAccResult">
		select
			amt_acc,
			code_acc,
			create_time,
			creator,
			credit_acc,
			del_flag,
			eu_status,
			modifier,
			modity_time,
			note,
			pk_pi,
			pwd,
			pk_piacc,
			unavailable_acc
		from psc_pi_acc
		where pk_piacc = #{pkPiacc}
	</select>

    <select id="getCount" parameterType="java.util.Map"
            resultType="java.lang.Integer">
        select count(1) from psc_pi_acc t1
        <include refid="whereConditions"/>
    </select>

    <select id="getList" resultType="cn.wellcare.acc.entity.PscPiAcc">
        select
        amt_acc,
        code_acc,
        create_time,
        creator,
        credit_acc,
        del_flag,
        eu_status,
        modifier,
        modity_time,
        note,
        pk_pi,
        pwd,
        pk_piacc,
        unavailable_acc
        from psc_pi_acc
        <include refid="whereConditions"/>
    </select>

    <!-- 查询条件 -->
    <sql id="whereConditions">
        where 1=1
        <trim suffixOverrides=",">
            <if test="amtAcc != null">
                and amt_acc= #{amtAcc}
            </if>
            <if test="codeAcc != null">
                and code_acc= #{codeAcc}
            </if>
            <if test="createTime != null">
                and create_time= #{createTime}
            </if>
            <if test="creator != null">
                and creator= #{creator}
            </if>
            <if test="creditAcc != null">
                and credit_acc= #{creditAcc}
            </if>
            <if test="delFlag != null">
                and del_flag= #{delFlag}
            </if>
            <if test="euStatus != null">
                and eu_status= #{euStatus}
            </if>
            <if test="modifier != null">
                and modifier= #{modifier}
            </if>
            <if test="modityTime != null">
                and modity_time= #{modityTime}
            </if>
            <if test="note != null">
                and note= #{note}
            </if>
            <if test="pkPi != null">
                and pk_pi= #{pkPi}
            </if>
            <if test="pkPiacc != null">
                and pk_piacc= #{pkPiacc}
            </if>
            <if test="unavailableAcc != null">
                and unavailable_acc = #{unavailableAcc}
            </if>
            <if test="pwd != null">
                and pwd = #{pwd}
            </if>
        </trim>
    </sql>

    <delete id="del">
        delete from psc_pi_acc where id = #{id}
	</delete>

    <update id="update" parameterType="cn.wellcare.acc.entity.PscPiAcc">
        update psc_pi_acc
        <set>
            amt_acc= #{amtAcc},
            code_acc= #{codeAcc},
            create_time= #{createTime},
            creator= #{creator},
            credit_acc= #{creditAcc},
            del_flag= #{delFlag},
            eu_status= #{euStatus},
            modifier= #{modifier},
            modity_time= #{modityTime},
            note= #{note},
            pk_pi= #{pkPi},
            pwd = #{pwd},
            pk_piacc= #{pkPiacc},
            unavailable_acc = #{unavailableAcc}
        </set>
        where pk_piacc = #{pkPiacc}
    </update>

    <insert id="save" parameterType="cn.wellcare.acc.entity.PscPiAcc" useGeneratedKeys="true" keyProperty="pkPiacc"
            keyColumn="pk_piacc">
		insert into 
			psc_pi_acc
		(
			amt_acc,
			code_acc,
			create_time,
			creator,
			credit_acc,
			del_flag,
			eu_status,
			modifier,
			modity_time,
			note,
			pk_pi,
			pwd,
			unavailable_acc
		)
		values
		(
			#{amtAcc},
			#{codeAcc},
			#{createTime},
			#{creator},
			#{creditAcc},
			#{delFlag},
			#{euStatus},
			#{modifier},
			#{modityTime},
			#{note},
			#{pkPi},
			#{pwd},
			#{unavailableAcc}
		)
	</insert>
    <insert id="batchInsertPscPiAcc" parameterType="java.util.List">
        insert into
        psc_pi_acc
        (
        amt_acc,
        code_acc,
        create_time,
        creator,
        credit_acc,
        del_flag,
        eu_status,
        modifier,
        modity_time,
        note,
        pk_pi,
        pwd,
        unavailable_acc
        )
        values
        <foreach collection="pscPiAccsList" item="list" index="index" separator=",">
            (
            #{list.amtAcc},
            #{list.codeAcc},
            #{list.createTime},
            #{list.creator},
            #{list.creditAcc},
            #{list.delFlag},
            #{list.euStatus},
            #{list.modifier},
            #{list.modityTime},
            #{list.note},
            #{list.pkPi},
            #{list.pwd},
            #{list.unavailableAcc}
            )
        </foreach>
    </insert>
    <update id="batchUpdatePscPiAcc" parameterType="java.util.List" >
    <foreach collection="pscPiAccsList" item="list" index="index" separator=";">
        update psc_pi_acc
        <set>
            <if test="list.amtAcc !=null">
                amt_acc = #{list.amtAcc,jdbcType=INTEGER},
            </if>
            <if test="list.codeAcc !=null">
                code_acc = #{list.codeAcc,jdbcType=INTEGER},
            </if>
            <if test="list.creditAcc !=null">
                credit_acc = #{list.creditAcc,jdbcType=INTEGER},
            </if>
            <if test="list.delFlag !=null">
                del_flag = #{list.delFlag,jdbcType=VARCHAR},
            </if>
            <if test="list.euStatus !=null">
                eu_status = #{list.euStatus,jdbcType=VARCHAR},
            </if>
            <if test="list.modifier !=null">
                modifier = #{list.modifier,jdbcType=VARCHAR},
            </if>
            <if test="list.modityTime !=null">
                modity_time = #{list.modityTime,jdbcType=DATE},
            </if>
            <if test="list.note !=null">
                note = #{list.note,jdbcType=VARCHAR},
            </if>
            <if test="list.pkPi !=null">
                pk_pi = #{list.pkPi,jdbcType=VARCHAR},
            </if>
            <if test="list.pwd !=null">
                pwd = #{list.pwd,jdbcType=VARCHAR},
            </if>
            <if test="list.unavailableAcc !=null">
                unavailable_acc = #{list.unavailableAcc,jdbcType=INTEGER}
            </if>
        </set>
        where pk_pi = #{list.pkPi,jdbcType=VARCHAR}
    </foreach>
    </update>
</mapper>