}*/

.m-leftmenu > li > ul:hover {
    display:block;
}

.m-leftmenu > li:hover > ul {
    display: block;
}

.m-leftmenu > li:hover > .expend {
    display: block;
}

.m-leftmenu > li > div > a.name {
    padding-left: 37px;
    width: 127px;
    display: block;
    /*width: 128px;*/
    /*height: 41px;*/
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 15px;
    font-family: simsum;
    left: 0px;
    z-index: 20;
    color: black;
}

.m-leftmenu > li > div {
    position: relative;
    width: 100%;
    height: 100%;

}

.m-leftmenu > li > div > a.up {
    background-image: url(../img/newup.png);
    left: 43px;
    width: 115px;
    background-repeat: no-repeat;
    z-index: 10;
    background-position: 88px 0px;
}

.m-leftmenu > li > div > a.icopic {
    position: absolute;
    display: inline-block;
    width: 14px;
    height: 14px;
    top: 9px;
    left: 14px;
    background-position: -14px 0px;

    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: -webkit-transform .9s;
    transition: -webkit-transform .9s;
    transition: transform .9s;
    transition: transform .9s,-webkit-transform .9s;
}

.m-leftmenu > li > div > a.down {
    /*background-image: url(../img/expand.jpg);*/
    left: 37px;
    width: 136px;
    background-repeat: no-repeat;
    z-index: 10;
    background-position: 88px 0px;
    padding-right: 21px;
    text-align: right;
    font-size: 13px;
    display: none;

}

.m-leftmenu > li > ul {
    /*padding-left: 30px;*/
    background-color: #ffffff;
    display: none;

    position: absolute;
    /*width: 161px;*/
    left: 198px;
    z-index: 30;
    border: 1px solid #1388af;
    top:-1px;
    /*bottom: 0px;*/
    /*overflow-y: auto;*/
}

.m-leftmenu > li  .moduletitle {
    height: 52px;
    font-size: 19px;
    line-height: 52px;
    text-indent: 35px;
}

.m-leftmenu > li > ul > ul {
    float: left;
}

.m-leftmenu > li > ul > ul > li {
    padding-left: 35px;
    height: 32px;
    line-height: 32px;
    font-size: 13px;
    cursor: pointer;
    overflow: hidden;
    position: relative;
    /*min-width: 201px;*/
    color: #777777;
}

.m-leftmenu > li > ul > ul > li > a.ico {
    display: inline;
    width: 4px;
    height: 4px;
    background-image: url(../img/shine-ico.png);
    position: absolute;
    left: 37px;
    top: 14px;
    background-position: -1px -1px;
    overflow: hidden;
}

.m-leftmenu > li > ul > ul > li > a.title {
    display: inline-block;
    /*width: 128px;*/
    height: 41px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    /*color: #222222;*/
    /*position: absolute;
    left: 30px;
    top: 0px;*/
    padding-right: 40px;
    font-size: 15px;
    min-width: 161px;
    font-family: simsum;
} 

.m-leftmenu > li > ul > ul > li:hover {
    background-color: #e8f2f5;
    color: #1388af;
}

.m-leftmenu > li > ul > ul > li:hover > a.ico {
    background-position: 5px -1px;
}

#nav4 > ul {
    margin: 0px;
