body {
    margin: 0;
    padding: 0;
    /* background-color: #aa2424; */
    font-family: Microsoft YaHei;
    background-image: url(../img/dotted.png);
}

.m-leftbar > .menus::-webkit-scrollbar {
    width: 8px;
    height: 8px;
}

.m-leftbar > .menus::-webkit-scrollbar-track-piece {
    background-color: #272e33;
    -webkit-border-radius: 6px;
}

.m-leftbar > .menus::-webkit-scrollbar-thumb:vertical {
    height: 5px;
    background-color: #bfc4c5;
    -webkit-border-radius: 6px;
}

.m-leftbar > .menus::-webkit-scrollbar-thumb:horizontal {
    width: 5px;
    background-color: #bfc4c5;
    -webkit-border-radius: 6px;
}

.m-leftbar > .menus {
    scrollbar-face-color: #bfc4c5;
    scrollbar-highlight-color: #272e33;
    scrollbar-arrow-color: #272e33;
}

ul,
li {
    margin: 0;
    padding: 0;
    list-style-type: none;
}

.g-top {
    height:80px;
    background-color: #204952;
    min-width: 1280px;
}

.g-head {
    height:80px;
    margin: 0 auto;
    background-image: url(../img/logo.png);
    background-repeat: no-repeat;
    background-position: 10px 2px;
    background-size: 200px 80px;
    position: relative;
    max-width: 1920px;
    min-width: 1280px;
}

.g-head .title {
    height:80px;
    line-height:80px;
    position: absolute;
    top: 0px;
    left: 230px;
    color: white;
    font-size: 29px;
}

.m-loguser {
    width: 180px;
    height:80px;
    position: absolute;
    top: 0px;
    right:53px;
}

.m-loguser > li {
    height:80px;
    line-height:80px;
    position: relative;
    float:right;
}
.m-loguser > li span{
	color:#ddd;
	font-size:14px;
	display:block;
	float:left;
}
.m-loguser > li a {
    display: block;
    background-repeat: no-repeat;
    cursor: pointer;
}
.m-loguser > li a.name {
    color: white;
    float: left;
    font-size: 14px;
    text-align: right;
    position: relative;
    left:10px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    height:80px;
}

.m-loguser > li a.detail {
    background-image: url(../img/detail.png);
    width: 17px;
    height: 12px;
    float: left;
    position: relative;
    top:34px;
    left:10px;
}

.m-loguser > li > ul {
    display: none; 
    position: absolute;
    width: 150px;
    height:60px;
    line-height:60px;
    background-color: #18343a;
    top:80px;
    left: 0px;
    z-index: 20;
}

.m-loguser > li > ul > li {
	color:#fff;
    text-align: center;
    cursor: pointer;
}

.m-loguser > li:active > ul {
    /*display: block;*/
}

.g-frame {
    min-width: 1280px;
    max-width: 1920px;
    min-height: 200px;
    margin: 0 auto;
    position: absolute;
    left: 0px;
    right: 0px;
    top: 0px;
    bottom: 0px;
}

.m-leftbar {
    width: 200px;
    position: fixed;
    top: 53px;
    bottom: 0px;
    z-index: 10;
    /* border: 1px solid #efefef; */
    /* background-image: url(../img/dotted.png); */
    background-color: #e8f2f5;
    -webkit-box-shadow: 3px 3px 3px #cbd6d6;
    -moz-box-shadow: 3px 3px 3px #cbd6d6;
    box-shadow: 3px 3px 3px #cbd6d6;
}

.m-leftbar .bar {
    height: 68px;
    background-color: #e8f2f5;
    line-height: 68px;
    text-align: left;
    font-size: 19px;
    padding: 0px 20px 0px 14px;
    color: black;
}

.m-leftbar .bar .expend {
    display: inline-block;
    width: 17px;
    height: 47px;
    position: absolute;
    right: -1px;
    border: 1px solid #d2e1ec;
    border-right-color: white;
    background-color: white;
    top:9px;
    background-image: url(../img/narrow.png);
    background-repeat: no-repeat;
    cursor: pointer;
}

.m-leftbar .bar .open {

}

.m-leftbar .bar .close {
    background-position: 0px -56px;
}

.m-leftbar .menus {
    position: absolute;
    top: 68px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    /* background-color: #e8f2f5; */
    /*overflow-y: auto;*/
    /*overflow-x: hidden;*/
}

.m-leftbar .menus .ico-expand {
    position: absolute;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
    background-image: url(../img/expand.png);
    background-repeat: no-repeat;
    background-position: center;
    background-color: #22272D;
    cursor: pointer;
}

.g-content {
    position: absolute;
    left: 45px;
    min-height: 200px;
    right: 0px;
    bottom: 0px;
    top: 0px;
}

.m-tablecontent {
    position: absolute;
    left: 8px;
    right: 8px;
    min-height: 200px;
    top: 0px;
    bottom: 0px;
    margin-top:7px;
}

.m-tablecontent .tableindex {
    height: 40px;
    line-height: 43px;
    background-image: url(../img/home.png);
    background-repeat: no-repeat;
    background-position-x: left;
    background-position-y: center;
    padding-left: 27px;
    font-size: 12px;
    color: #8F9190;
    background-position: 0px 12px;
}

.m-tablecontent .tablecontent {
    /*min-height: 150px;
    position: absolute;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;*/
    width: 100%;
    height: 100%;
    position: relative;
    padding-top: 2px;
}

#main {
    /*position: absolute;
    top: 33px;
    bottom: 4px;
    left: 0px;
    right: 0px;
