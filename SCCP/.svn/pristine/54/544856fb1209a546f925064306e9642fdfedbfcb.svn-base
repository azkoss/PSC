.m-leftmenu {
    color: #222222;
    /*display: none;*/
    position: absolute;
    left: 0px;
    right: 0px;
    top: 0px;
    z-index: 10;
    padding: 1px 0px 1px 0px;
}

.m-leftmenu > li {
    /*padding-left: 43px;*/
    height: 33px;
    line-height: 33px;
    font-size: 14px;
    /* background-color: #e8f2f5; */
    cursor: pointer;
    /*background-image: url(../img/newup.png);*/
    background-repeat: no-repeat;
    position: relative;
    /*border-top-width:1px;
    border-bottom-width: 1px;
    border-color: #e8f2f5;*/
    border-top: 1px solid #e8f2f5;
    border-bottom: 1px solid #e8f2f5;
    border-left: 1px solid #e8f2f5;
}

.m-leftmenu > li:hover {
    background-color: #ffffff;
    border-color: #1388af;
}

.m-leftmenu > li:hover > div {
    background-color: #ffffff;
    z-index:100;
} 

.m-leftmenu > li:hover > div > a.name {
    color: black;
}

.m-leftmenu > li:hover > div > a.ico {
    color: #1388af;
}

.m-leftmenu > li:hover > div > a.icopic {
    background-position: 0px 0px;
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg)
}

.m-leftmenu > li > div > a {
    display: block;
    height: 33px;
    position: absolute;
}

/*.m-leftmenu > li > a.ico {
    left: 0px;
    width: 163px;
    background-repeat: no-repeat;
    z-index: 10;
}*/

/*.m-leftmenu > li > a.ico:hover + a + a + ul {
